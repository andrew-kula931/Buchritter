generator client {
  provider      = "prisma-client-js"
  output        = "./"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Documents {
  id         Int     @id @default(autoincrement())
  userId     Int
  parentId   Int?
  type       String
  name       String
  created_at String
  updated_at String?
  body       Json?
}
